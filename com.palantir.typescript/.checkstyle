<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="false" sync-formatter="false">
  <local-check-config name="TypeScript Checks" location="checks.xml" type="project" description="">
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="TypeScript Checks" enabled="true" check-config-name="TypeScript Checks" local="true">
    <file-match-pattern match-pattern=".java$" include-pattern="true"/>
    <file-match-pattern match-pattern=".properties$" include-pattern="true"/>
    <file-match-pattern match-pattern=".ts$" include-pattern="true"/>
    <file-match-pattern match-pattern=".xml$" include-pattern="true"/>
    <file-match-pattern match-pattern="bin/.*" include-pattern="false"/>
    <file-match-pattern match-pattern="TSBridge/lib/.*" include-pattern="false"/>
  </fileset>
  <filter name="NonSrcDirs" enabled="false"/>
</fileset-config>
